        "cross-env": "^5.2.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.5",
        "object-path": "^0.11.5",
        "popper.js": "^1.16.0",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "ssri": "^8.0.1",
        "ts-loader": "^9.4.1",
        "typescript": "^4.9.3",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
