        "axios": "^0.21.1",
        "bootstrap": "^5.1.3",
        "cross-env": "^5.2.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^6.0.43",
        "lodash": "^4.17.5",
        "object-path": "^0.11.5",
        "popper.js": "^1.16.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "ssri": "^8.0.1",
